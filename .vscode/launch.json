{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}",
      "justMyCode": false
    },
    {
        "name": "Debug auth",
        "type": "debugpy",
        "request": "launch",
        "program": "${workspaceFolder}/bin/main.py",
        "console": "integratedTerminal",
        "args": ["auth"]
    },
    {
      "name": "Debug invoice-list",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "list", "60", "19507820", "--filter", "P"]
    },
    {
      "name": "Debug invoice-download",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "download", "59", "19507820", "--filter", "P"]
    },
    {
      "name": "Debug invoice-process",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "process"],
      "justMyCode": false
    }
]
}
