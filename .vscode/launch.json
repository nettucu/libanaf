{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": "${command:pickArgs}",
      "justMyCode": false
    },
    {
      "name": "Debug auth",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["auth"]
    },
    {
      "name": "Debug invoice-list",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "list", "60", "19507820", "--filter", "P"],
      "justMyCode": false
    },
    {
      "name": "Debug invoice-download",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "download", "59", "19507820", "--filter", "P"]
    },
    {
      "name": "Debug invoice-process",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "args": ["invoices", "process"],
      "justMyCode": false
    },
    {
      "name": "Debug invoice-show",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "env": {
        "LIBANAF_CONFIG_FILE": "${workspaceFolder}/conf/config.toml",
        "LIBANAF_LOGGING_CONFIG_FILE": "${workspaceFolder}/conf/logging_py.json",
        "LIBANAF_SECRETS_PATH": "${workspaceFolder}/conf/secrets"
      },
      "args": ["invoices", "show", "-i", "JAS018779130"],
      "justMyCode": false
    },
    {
      "name": "Debug invoice-summary",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "env": {
        "LIBANAF_CONFIG_FILE": "${workspaceFolder}/conf/config.toml",
        "LIBANAF_LOGGING_CONFIG_FILE": "${workspaceFolder}/conf/logging_py.json",
        "LIBANAF_SECRETS_PATH": "${workspaceFolder}/conf/secrets"
      },
      "args": ["invoices", "summary", "-s", "hd dental repair"],
      "justMyCode": false
    },
    {
      "name": "Debug prod-summary",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/bin/main.py",
      "console": "integratedTerminal",
      "env": {
        "LIBANAF_CONFIG_FILE": "${workspaceFolder}/conf/config.toml",
        "LIBANAF_LOGGING_CONFIG_FILE": "${workspaceFolder}/conf/logging_py.json",
        "LIBANAF_SECRETS_PATH": "${workspaceFolder}/conf/secrets"
      },
      "args": ["invoices", "prod-summary", "-i", "BTER1006066"],
      "justMyCode": false
    }
  ]
}
